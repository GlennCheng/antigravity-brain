{
  "name": "antigravity-brain",
  "displayName": "Antigravity Brain: Visual File Graph",
  "description": "Transform your Markdown files & tasks into an interactive Mind Map. A Force-Directed Graph extension for VS Code.",
  "version": "0.0.1",
  "publisher": "antigravity",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "antigravity-brain.openGraph",
        "title": "Antigravity Brain: Open Graph"
      },
      {
        "command": "antigravity-brain.viewHistory",
        "title": "View File History",
        "icon": "$(history)"
      },
      {
        "command": "antigravity-brain.searchBrains",
        "title": "Antigravity Brain: Search",
        "icon": "$(search)"
      },
      {
        "command": "antigravity-brain.recentActivity",
        "title": "Antigravity Brain: Recent Activity",
        "icon": "$(clock)"
      },
      {
        "command": "antigravity-brain.refreshTree",
        "title": "Antigravity Brain: Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "antigravity-brain.locateInTree",
        "title": "Antigravity Brain: Locate File in Tree",
        "icon": "$(target)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "antigravity-explorer",
          "title": "Antigravity Brain",
          "icon": "resources/brain.svg"
        }
      ]
    },
    "views": {
      "antigravity-explorer": [
        {
          "id": "antigravity-files",
          "name": "Brain Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "antigravity-brain.locateInTree",
          "when": "view == antigravity-files",
          "group": "navigation@0"
        },
        {
          "command": "antigravity-brain.searchBrains",
          "when": "view == antigravity-files",
          "group": "navigation@1"
        },
        {
          "command": "antigravity-brain.recentActivity",
          "when": "view == antigravity-files",
          "group": "navigation@2"
        },
        {
          "command": "antigravity-brain.refreshTree",
          "when": "view == antigravity-files",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "antigravity-brain.viewHistory",
          "when": "view == antigravity-files && viewItem == fileWithHistory",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@types/dagre": "^0.7.53",
    "@types/diff": "^7.0.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "css-loader": "^7.1.3",
    "dagre": "^0.8.5",
    "diff": "^8.0.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "reactflow": "^11.11.4",
    "style-loader": "^4.0.0"
  }
}
